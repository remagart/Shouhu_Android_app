<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
    <size android:height="200dp" android:width="200dp"/>
    <!-- 設置一个顏色邊框 -->
    <stroke
        android:width="10px"
        android:color="#ff9611"
        android:dashGap="5px"
        android:dashWidth="50px"/>
    <!-- 設置一下邊距,讓空間大一點 -->
    <padding
        android:bottom="30dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp" />

</shape>